{
  "name": "emoji-database",
  "version": "0.2.0",
  "description": "Evergreen emoji datasets and regex patterns.",
  "main": "./lib/index.js",
  "scripts": {
    "babel": "build-lib ./src -d ./lib && cp ./flow/emoji-database.js.flow ./lib",
    "coverage": "yarn run jest -- --coverage",
    "eslint": "run-linter ./bin ./src ./tests",
    "flow": "type-check",
    "generate": "yarn run babel && node ./bin/generate-data.js && node ./bin/generate-regex.js",
    "generate:table": "node ./bin/build-filesize-table.js",
    "jest": "run-tests",
    "posttest": "yarn run flow",
    "pretest": "yarn run eslint",
    "preversion": "yarn test && yarn run babel",
    "test": "yarn run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/emoji-database.git"
  },
  "keywords": [
    "emoji",
    "data",
    "dataset",
    "database",
    "regex"
  ],
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/emoji-database/issues"
  },
  "homepage": "https://github.com/milesj/emoji-database#readme",
  "devDependencies": {
    "@milesj/build-tool-config": "^0.0.7",
    "chalk": "^1.1.3",
    "diacritics": "^1.3.0",
    "emojione": "^3.0.3",
    "filesize": "^3.5.9",
    "glob": "^7.1.1",
    "gzip-size": "^3.0.0",
    "lodash": "^4.17.4",
    "regexgen": "^1.2.3",
    "unicode-emoji-data": "^3.0.0"
  },
  "babel": {
    "extends": "./node_modules/@milesj/build-tool-config/babel.json5"
  }
}
