{
  "name": "emoji-database",
  "version": "0.4.0",
  "description": "Evergreen emoji datasets and regex patterns.",
  "main": "./lib/index.js",
  "scripts": {
    "babel": "build-lib ./src -d ./lib",
    "coverage": "run-coverage",
    "eslint": "run-linter ./bin ./src ./tests",
    "flow": "type-check",
    "generate": "yarn run babel && yarn run generate:data && yarn run generate:regex && yarn run generate:table",
    "generate:data": "node ./lib/bin/generate-data.js",
    "generate:regex": "node ./lib/bin/generate-regex.js",
    "generate:table": "node ./lib/bin/build-filesize-table.js",
    "jest": "NODE_ENV=test run-tests",
    "posttest": "yarn run flow",
    "pretest": "yarn run eslint",
    "preversion": "yarn test && yarn run babel",
    "test": "yarn run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/emoji-database.git"
  },
  "keywords": [
    "emoji",
    "data",
    "dataset",
    "database",
    "regex"
  ],
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/emoji-database/issues"
  },
  "homepage": "https://github.com/milesj/emoji-database#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "diacritics": "^1.3.0",
    "emojione": "^3.0.0",
    "lodash": "^4.17.0",
    "unicode-emoji-data": "^3.0.0"
  },
  "devDependencies": {
    "@milesj/build-tool-config": "^0.8.0",
    "chalk": "^1.1.3",
    "filesize": "^3.5.10",
    "glob": "^7.1.2",
    "gzip-size": "^3.0.0",
    "regexgen": "^1.2.3"
  },
  "babel": {
    "extends": "./node_modules/@milesj/build-tool-config/babel.node.json5"
  },
  "eslintConfig": {
    "extends": "./node_modules/@milesj/build-tool-config/eslint.node.json5"
  },
  "jest": {
    "preset": "@milesj/build-tool-config"
  }
}
